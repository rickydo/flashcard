<div class="set_index">	
	<ul class="thumbs">
		<% @flashcard_sets.each do |set| %>
				<li class = "deck_index">
					<p class="index_title"><%= link_to set.name , flashcard_set_path(set), class: "pop" %></p>
					<a href="/flashcard_set/<%= set.id %>"><img class="buzz-out" src="<%=set.image_url%>"></a><br>
				</li>
		<% end %>
	</ul>
</div>